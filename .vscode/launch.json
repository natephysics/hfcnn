    "version": "0.2.0",
    "configurations": [
    

        {
            "name": "Run Project",
            "type": "python",
            "request": "launch",
            "program": "hfcnn",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            },
            // "args": ["hydra.run.dir=."],
        },
        {
            "name": "Run Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Exp: Overfit",
            "type": "python",
            "request": "launch",
            "program": "hfcnn",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["experiment=overfit"],
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Exp: Prepare Data",
            "type": "python",
            "request": "launch",
            "program": "hfcnn",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["action=prepare_data"],
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Exp: Prepare Test",
            "type": "python",
            "request": "launch",
            "program": "hfcnn",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["action=prepare_test"],
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Exp: Ax",
            "type": "python",
            "request": "launch",
            "program": "hfcnn",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["--multirun", "experiment=ax"],
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Exp: Half Res",
            "type": "python",
            "request": "launch",
            "program": "hfcnn",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["experiment=reduce_dim"],
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Exp: Incept",
            "type": "python",
            "request": "launch",
            "program": "hfcnn",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": ["experiment=incept"],
            "env": { 
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
